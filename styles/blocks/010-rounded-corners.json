{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "rounded-corners",
	"title": "Rounded Corners",
	"blockTypes": [ "core/group", "core/columns", "core/column", "core/comments", "core/code" ],
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--neutral)",
			"text": "var(--wp--preset--color--very-dark-gray)"
		},
		"border": {
			"radius": "var(--wp--custom--border--radius)",
			"style": "none"
		},
		"spacing": {
			"padding": "var:preset|spacing|40"
		},
		"css": "overflow: hidden"
	}
}
