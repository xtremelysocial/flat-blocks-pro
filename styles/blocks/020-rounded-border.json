{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "rounded-border",
	"title": "Border",
	"blockTypes": [ "core/group", "core/columns", "core/column", "core/cover", "core/media-text", "core/comments" ],
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--neutral)",
			"text": "var(--wp--custom--color--contrast--neutral, var(--wp--custom--color--contrast--default-light, var(--wp--preset--color--contrast)))"
		},
		"border": {
			"style": "solid",
			"width": "1px",
			"color": "var(--wp--custom--color--border)",
			"radius": "var(--wp--custom--border--radius)"
		},
		"spacing": {
			"padding": "var:preset|spacing|40"
		},
		"css": ".is-layout-flex { overflow: hidden }"
	}
}
