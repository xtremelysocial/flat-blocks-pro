{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "thick-rounded-border",
	"title": "Thick Border",
	"blockTypes": [ "core/group", "core/columns", "core/column", "core/cover", "core/media-text", "core/comments", "core/code" ],
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--neutral)",
			"text": "var(--wp--preset--color--very-dark-gray)"
		},
		"border": {
			"style": "solid",
			"width": "3px",
			"color": "var(--wp--custom--border--color)",
			"radius": "var(--wp--custom--border--radius)"
		},
		"spacing": {
			"padding": "var:preset|spacing|60"
		},
		"css": ".is-layout-flex { overflow: hidden }"
	}
}
