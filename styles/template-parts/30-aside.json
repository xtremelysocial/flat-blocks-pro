{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "section-aside",
	"title": "Aside",
	"blockTypes": [ "core/group", "core/comments" ],
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--neutral)",
			"text": "var(--wp--custom--color--contrast--neutral, var(--wp--custom--color--contrast--default-light, var(--wp--preset--color--contrast)))"
		},
		"border": {
			"style": "solid",
			"color": "var(--wp--custom--color--border)",
			"radius": "var(--wp--custom--border--radius)",
			"width": "1px"
		},
		"shadow": "none",
		"spacing": {
			"padding": {
				"top": "var(--wp--preset--spacing--40)",
				"right": "var(--wp--preset--spacing--40)",
				"bottom": "var(--wp--preset--spacing--40)",
				"left": "var(--wp--preset--spacing--40)"
			},
			"margin": {
				"top": "var(--wp--preset--spacing--40)",
				"bottom": "var(--wp--preset--spacing--40)"
			}
		}
	}
}
