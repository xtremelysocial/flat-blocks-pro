{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "section-footer",
	"title": "Footer",
	"blockTypes": [ "core/group" ],
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--dark)",
			"text": "var(--wp--custom--color--contrast--dark, var(--wp--custom--color--contrast--default-black, var(--wp--preset--color--foreground-alt)))"
		},
		"shadow": "none",		
		"spacing": {
			"padding": {
				"top": "var(--wp--preset--spacing--60)",
				"bottom": "var(--wp--preset--spacing--60)"
			},
			"margin": {
				"top": "0",
				"bottom": "0"
			}
		}
	}	
}
