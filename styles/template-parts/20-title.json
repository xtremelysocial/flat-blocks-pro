{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "section-title",
	"title": "Title",
	"blockTypes": [ "core/group" ],
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--primary)",
			"text": "var(--wp--custom--color--contrast--primary, var(--wp--custom--color--contrast--default-dark, var(--wp--preset--color--foreground-alt)))"
		},
		"shadow": "none",
		"spacing": {
			"padding": {
				"top": "var(--wp--preset--spacing--60)",
				"bottom": "var(--wp--preset--spacing--60)"
			},
			"margin": {
				"top": "0",
				"bottom": "0"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--off-white)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--off-white)"
					}
				}
			}
		}
	}
}
