{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "title": "Pewter",
    "categories": [
        "vibrant",
        "warm",
        "feminine",
        "modern",
        "portfolio"
    ],
    "settings": {
        "color": {
            "palette": [
                {
                    "color": "#fdfafa",
                    "name": "Color 1",
                    "slug": "base"
                },
                {
                    "color": "#d4bbbb",
                    "name": "Color 2",
                    "slug": "tertiary"
                },
                {
                    "color": "#01616B",
                    "name": "Color 4",
                    "slug": "secondary"
                },
                {
                    "color": "#716B66",
                    "name": "Color 6",
                    "slug": "theme-6"
                },
                {
                    "color": "#181515",
                    "name": "Color 5",
                    "slug": "contrast"
                },
                {
                    "color": "#f7ecec",
                    "name": "Color 3",
                    "slug": "primary"
                }
            ],
            "duotone": [
                {
                    "colors": [
                        "#01616B",
                        "#d4bbbb"
                    ],
                    "slug": "duotone-1",
                    "name": "Duotone 1"
                },
                {
                    "colors": [
                        "#181515",
                        "#01616B"
                    ],
                    "slug": "duotone-2",
                    "name": "Duotone 2"
                }
            ],
            "gradients": [
                {
                    "slug": "gradient-text-transparent",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--contrast) -50%, transparent 50%)",
                    "name": "Text to Transparent"
                },
                {
                    "slug": "gradient-1-2",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--base) 0%, var(--wp--preset--color--tertiary) 100%)",
                    "name": "Gradient 1"
                },
                {
                    "slug": "gradient-1-3",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--base) 0%, var(--wp--preset--color--secondary) 100%)",
                    "name": "Gradient 2"
                },
                {
                    "slug": "gradient-1-4",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--base) 0%, var(--wp--preset--color--theme-6) 100%)",
                    "name": "Gradient 3"
                },
                {
                    "slug": "gradient-1-5",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--base) 0%, var(--wp--preset--color--contrast) 100%)",
                    "name": "Gradient 4"
                },
                {
                    "slug": "gradient-1-6",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--base) 0%, var(--wp--preset--color--primary) 100%)",
                    "name": "Gradient 5"
                },
                {
                    "slug": "gradient-2-3",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--tertiary) 0%, var(--wp--preset--color--secondary) 100%)",
                    "name": "Gradient 6"
                },
                {
                    "slug": "gradient-2-4",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--tertiary) 0%, var(--wp--preset--color--theme-6) 100%)",
                    "name": "Gradient 7"
                },
                {
                    "slug": "gradient-2-5",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--tertiary) 0%, var(--wp--preset--color--contrast) 100%)",
                    "name": "Gradient 8"
                },
                {
                    "slug": "gradient-2-6",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--tertiary) 0%, var(--wp--preset--color--primary) 100%)",
                    "name": "Gradient 9"
                },
                {
                    "slug": "gradient-3-4",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--secondary) 0%, var(--wp--preset--color--theme-6) 100%)",
                    "name": "Gradient 10"
                },
                {
                    "slug": "gradient-3-5",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--secondary) 0%, var(--wp--preset--color--contrast) 100%)",
                    "name": "Gradient 11"
                },
                {
                    "slug": "gradient-3-6",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--secondary) 0%, var(--wp--preset--color--primary) 100%)",
                    "name": "Gradient 12"
                },
                {
                    "slug": "gradient-4-5",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--theme-6) 0%, var(--wp--preset--color--contrast) 100%)",
                    "name": "Gradient 13"
                },
                {
                    "slug": "gradient-4-6",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--theme-6) 0%, var(--wp--preset--color--primary) 100%)",
                    "name": "Gradient 14"
                },
                {
                    "slug": "gradient-5-6",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--contrast) 0%, var(--wp--preset--color--primary) 100%)",
                    "name": "Gradient 15"
                }
            ]
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--base)",
            "text": "var(--wp--preset--color--contrast)"
        },
        "blocks": {
            "core/separator": {
                "color": {
                    "background": "var(--wp--preset--color--tertiary)",
                    "text": "var(--wp--preset--color--tertiary)"
                }
            }
        },
        "elements": {
            "heading": {
                "color": {
                    "text": "var(--wp--preset--color--contrast)"
                }
            },
            "link": {
                "color": {
                    "text": {
                        "ref": "styles.color.text"
                    }
                }
            }
        },
        "variations": {
            "section-1": {
                "color": {
                    "background": "var(--wp--preset--color--tertiary)",
                    "text": "var(--wp--preset--color--contrast)"
                },
                "blocks": {
                    "core/paragraph": {
                        "elements": {
                            "link": {
                                "color": {
                                    "text": "var(--wp--preset--color--contrast)"
                                }
                            }
                        }
                    },
                    "core/separator": {
                        "color": {
                            "background": "var(--wp--preset--color--contrast)",
                            "text": "var(--wp--preset--color--contrast)"
                        }
                    }
                },
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--contrast)"
                        }
                    },
                    "button": {
                        "color": {
                            "background": "var(--wp--preset--color--secondary)",
                            "text": "var(--wp--preset--color--base)"
                        },
                        ":hover": {
                            "color": {
                                "background": "var(--wp--preset--color--contrast)",
                                "text": "var(--wp--preset--color--base)"
                            }
                        }
                    },
                    "heading": {
                        "color": {
                            "text": "var(--wp--preset--color--contrast)"
                        }
                    }
                }
            },
            "section-2": {
                "color": {
                    "background": "var(--wp--preset--color--theme-6)",
                    "text": "var(--wp--preset--color--base)"
                },
                "blocks": {
                    "core/paragraph": {
                        "elements": {
                            "link": {
                                "color": {
                                    "text": "var(--wp--preset--color--base)"
                                }
                            }
                        }
                    },
                    "core/separator": {
                        "color": {
                            "background": "var(--wp--preset--color--base)",
                            "text": "var(--wp--preset--color--base)"
                        }
                    }
                },
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--base)"
                        }
                    },
                    "button": {
                        "color": {
                            "background": "var(--wp--preset--color--secondary)",
                            "text": "var(--wp--preset--color--base)"
                        },
                        ":hover": {
                            "color": {
                                "background": "var(--wp--preset--color--contrast)",
                                "text": "var(--wp--preset--color--base)"
                            }
                        }
                    },
                    "heading": {
                        "color": {
                            "text": "var(--wp--preset--color--base)"
                        }
                    }
                }
            },
            "section-3": {
                "color": {
                    "background": "var(--wp--preset--color--contrast)",
                    "text": "var(--wp--preset--color--tertiary)"
                },
                "blocks": {
                    "core/paragraph": {
                        "elements": {
                            "link": {
                                "color": {
                                    "text": "var(--wp--preset--color--tertiary)"
                                }
                            }
                        }
                    },
                    "core/separator": {
                        "color": {
                            "background": "var(--wp--preset--color--tertiary)",
                            "text": "var(--wp--preset--color--tertiary)"
                        }
                    }
                },
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--primary)"
                        }
                    },
                    "button": {
                        "color": {
                            "background": "var(--wp--preset--color--secondary)",
                            "text": "var(--wp--preset--color--base)"
                        },
                        ":hover": {
                            "color": {
                                "background": "var(--wp--preset--color--primary)",
                                "text": "var(--wp--preset--color--contrast)"
                            }
                        }
                    },
                    "heading": {
                        "color": {
                            "text": "var(--wp--preset--color--primary)"
                        }
                    }
                }
            }
        }
    }
}
