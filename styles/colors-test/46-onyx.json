                    "name": "Gradient 7"
                },
                {
                    "slug": "gradient-3-4",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--contrast) 0%, var(--wp--preset--color--secondary) 100%)",
                    "name": "Gradient 8"
                },
                {
                    "slug": "gradient-3-5",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--contrast) 0%, var(--wp--preset--color--primary) 100%)",
                    "name": "Gradient 9"
                },
                {
                    "slug": "gradient-4-5",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--secondary) 0%, var(--wp--preset--color--primary) 100%)",
                    "name": "Gradient 10"
                }
            ]
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--base)",
            "text": "var(--wp--preset--color--contrast)"
        },
        "blocks": {
            "core/separator": {
                "color": {
                    "text": "var(--wp--preset--color--contrast)"
                }
            },
            "core/navigation": {
                "color": {
                    "text": "var(--wp--preset--color--contrast)"
                }
            },
            "core/cover": {
                "color": {
                    "text": "var(--wp--preset--color--secondary)"
                },
                "elements": {
