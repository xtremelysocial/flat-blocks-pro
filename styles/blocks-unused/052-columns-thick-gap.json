{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "thick-gap",
	"title": "Thick Gap",
	"blockTypes": [ "core/columns" ],
	"styles": {
		"css": ".wp-block-columns-is-layout-flex, .wp-block-columns-is-layout-grid { gap: var(--wp--preset--spacing--60) }",
		"spacing": {
			"blockGap": "var:preset|spacing|60"
		}
	}
}
