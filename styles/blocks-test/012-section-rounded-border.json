{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "rounded-border",
	"title": "Border",
	"blockTypes": [ "core/group", "core/column", "core/comments", "core/post-comments" ],
	"styles": {
		"border": {
			"style": "solid",
			"width": "1px",
			"color": "var(--wp--custom--border--color)",
			"radius": "var(--wp--custom--border--radius)"
		},
		"spacing": {
			"padding": "var:preset|spacing|40"
		}
	}
}
