{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "thick-rounded-border",
	"title": "Thick Border",
	"blockTypes": [ "core/group" ],
	"styles": {
		"css": ".is-layout-flex { overflow: hidden }",
		"border": {
			"style": "solid",
			"width": "3px",
			"color": "var(--wp--custom--border--color)",
			"radius": "var(--wp--custom--border--radius)"
		},
		"spacing": {
			"padding": "var:preset|spacing|60"
		}
	}
}
