{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Default - System & Style Script",
	"settings": {
		"typography": {
			"fontSizes": [
				{
					"slug": "extra-small",
					"size": "14px",
					"name": "Extra Small"
				},
				{
					"slug": "small",
					"size": "16px",
					"name": "Small"
				},
				{
					"slug": "normal",
					"size": "18px",
					"name": "Normal"
				},
				{
					"slug": "medium",
					"size": "24px",
					"name": "Medium (h4)"
				},
				{
					"slug": "large",
					"size": "clamp(28px, 6vw, 32px) /*clamp(24px, 6vw, 28px)*/",
					"name": "Large (h3)"
				},
				{
					"slug": "larger",
					"size": "clamp(34px, 7vw, 40px) /*clamp(30px, 7vw, 34px)*/",
					"name": "Larger (h2)"
				},
				{
					"slug": "x-large",
					"size": "clamp(48px, 10vw, 52px) /*clamp(36px, 10vw, 48px)*/",
					"name": "Extra Large (h1)"
				},
				{
					"slug": "huge",
					"size": "clamp(54px, 12vw, 62px) /*clamp(50px, 12vw, 54px)*/",
					"name": "Huge"
				},
				{
					"slug": "gigantic",
					"size": "clamp(64px, 14vw, 68px) /*clamp(56px, 14vw, 64px)*/",
					"name": "Gigantic"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)"
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--style-script)"
				}
			}
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--style-script)"
				}
			},
			"h5": {
				"typography": {
					"textTransform": "none"
				}
			},
			"h6": {
				"typography": {
					"textTransform": "none"
				}
			}
		}
	}
}